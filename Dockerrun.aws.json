{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "nginx",
        "image": "markwbennett/ravel-platform_nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["client", "server"],
        "memory": 128
      },
      {
        "name": "client",
        "image": "markwbennett/ravel-platform_client",
        "hostname": "client",
        "essential": false,
        "memory": 256
      },
      {
        "name": "server",
        "image": "markwbennett/ravel-platform_server",
        "hostname": "server",
        "essential": false,
        "memory": 256
      }
    ]
}